
<web-app>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>Register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Initialdepo</servlet-name>
    <servlet-class>Initialdepo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Displaybal</servlet-name>
    <servlet-class>Displaybal</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Bankstatement</servlet-name>
    <servlet-class>Bankstatement</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Withdraw</servlet-name>
    <servlet-class>Withdraw</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Deposit</servlet-name>
    <servlet-class>Deposit</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>Logout</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Initialdepo</servlet-name>
    <url-pattern>/initialdepo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Displaybal</servlet-name>
    <url-pattern>/displaybal</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Bankstatement</servlet-name>
    <url-pattern>/bankstatement</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Withdraw</servlet-name>
    <url-pattern>/withdraw</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Deposit</servlet-name>
    <url-pattern>/deposit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
</web-app>
